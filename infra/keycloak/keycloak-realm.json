{
  "realm": "Elsewhere",
  "enabled": true,
  "roles": {
    "realm": [
      {
        "name": "traveler"
      },
      {
        "name": "creator"
      },
      {
        "name": "admin"
      }
    ]
  },
  "clients": [
    {
      "clientId": "elsewhere-web",
      "publicClient": true,
      "redirectUris": [
        "http://localhost:3000/*"
      ],
      "webOrigins": [
        "*"
